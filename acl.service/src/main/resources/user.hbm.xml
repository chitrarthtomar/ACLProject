<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
<hibernate-mapping>  
<class name="model.User" table="User">  
          <id name="uId">  
          <generator class="assigned"></generator>  
          </id>  
            
          <property name="uName"></property>  
          <property name="uPassword"></property> 
          <property name = "uRole"></property>
          <property name="uMandatoryAttributes"></property>
          <property name="uArbitraryAttributes"> </property>
          <property name="uResource"></property>
</class>  
            
</hibernate-mapping> 
